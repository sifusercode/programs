       void drawPenguin(){
           System.out.println("+-------------------------------------+");
           System.out.println("                PENGUIN                ");
           System.out.println("+-------------------------------------+");
           System.out.println("         /~~~~~~L");
           System.out.println("        /'    -s- ~~~~L");
           System.out.println("       /'dHHb      ~~~~");
           System.out.println("      /'dHHHA     :");
           System.out.println("     /' VHHHHaadHHb:");
           System.out.println("    /'   `VHHHHHHHHb:");
           System.out.println("   /'      `VHHHHHHH:");
           System.out.println("  /'        dHHHHHHH:");
           System.out.println("  |        dHHHHHHHH:");
           System.out.println("  |       dHHHHHHHH:");
           System.out.println("  |       VHHHHHHHHH:");
           System.out.println("  |   b    HHHHHHHHV:");
           System.out.println("  |   Hb   HHHHHHHV'");
           System.out.println("  |   HH  dHHHHHHV'");
           System.out.println("  |   VHbdHHHHHHV'");
           System.out.println("  |    VHHHHHHHV'");
           System.out.println("   L    VHHHHHHH:");
           System.out.println("    LoodboooooodH");
           System.out.println("HHHHHHHHHHHHHHHHHHHHHHHHHHHHHHHHHHHHHHHH");
           System.out.println("");
       }

       void drawGiraffe(){
           System.out.println("+---------------------------------------------------------------+");
           System.out.println("                             GIRAFFE                             ");
           System.out.println("+---------------------------------------------------------------+");
           System.out.println("              oo _,        ,");
           System.out.println("             _((-'_/       |");
           System.out.println("            /      '',   _//__,     __/");
           System.out.println("          ,',O`  | #  ',/.---'     /,-'");
           System.out.println("        ,'    __/-.   # '       L.//");
           System.out.println("   |`._(;_,--'     L     '       ||");
           System.out.println("    `-.L           ,L #   ',     ||");
           System.out.println("      _LL         || L   #  '    ||");
           System.out.println("    /,-.`L       //   L      '  //");
           System.out.println("   //   `.`.    ||   | L #  # ',/");
           System.out.println("   |      `.`._// L //  L     # ',");
           System.out.println("            `-.:L  ||    L  #     ',");
           System.out.println("   L`.___     // L:L LL     L  #  #   ',");
           System.out.println(",--'----.L  ||   L:L ||   //L         #',");
           System.out.println("        LL//    | | ||  //  L#    #     ',_");
           System.out.println("         |:|    |:|  LL//    L  #    #     `--.__");
           System.out.println("        /:/     |:|   LL      L     #    #     # `--._");
           System.out.println("       / /      | |    ||     |#      #          #    `-.");
           System.out.println("L-     /:/    /| | |    ||     ;   #       #   #        # L");
           System.out.println(" LL__ |:|   _//  |:|    ||    :     #   #       #   #      L");
           System.out.println(" `-.L| |  /,-   |:|   //     :  #    #      #     #  /  #  :");
           System.out.println("    L :| //     |:|  //_     : # |  #               /    # |L");
           System.out.println("     L:|//      L L_/ / LL   :   :        |#     # | #     |LL");
           System.out.println("     |: /    /|  L ::/   `    L # L   #   |  #     |      #| LL");
           System.out.println("     / / L_ //   | : |         L  #L#  #  |       ,|  #    | ||");
           System.out.println("    | |    ||   / ::/           |   L     |# __,.' L    #  | ||");
           System.out.println("    |:|     LL /:: /            | #/ |#  /-'' `. #  L     #; ||");
           System.out.println("     L:L     LL:::/             |  | |  |       L  # L #  /  ||");
           System.out.println("     | |     / ::/              |  | | #|        |  | |  /   ||");
           System.out.println("     | |     | : |              |# | |  |        |  | |# |   ;;");
           System.out.println("     |:|    / ::/               |  | | #|        |# | |  |  .;;.");
           System.out.println("      L:L   |:::|               |  | |  |        |  | | #|  ;;;;");
           System.out.println("      |:|  /:: /                | #| |  |        | #| |  |  ;;;;");
           System.out.println("      |:| / : / _  _            |  | |# |  __   _(  ) (  )  ;;;;");
           System.out.println("      | |L|   |/ //  _    _    _(  ) (  ) _ LLL//| / L| /  _ _");
           System.out.println("      | :L|:: |/ ////_ _  _LL / _L |  L | _L L|//| |LL| |L/// _ __");
           System.out.println("       L :| :_|/ /// _  LL LL|///| |  | |_LLLL |/| |LL| |L//////_");
           System.out.println("     _ | _  / |_//     LL LLL|///| |  | | LL L||_| |_L|_|L/ // /");
           System.out.println("  _  _L _ L//:/  _  _ _ L_L_LL/_/| |  | | __   _ LLL L/ |/  _");
           System.out.println("_  LL LL L|||/_ / _  L L/// _  _LL_|  | |  _  _ LL L|||/ //  _");
           System.out.println("  LL LLLLL_|LLL//_ LLL L|/_  LL LLLLL//_|//   _LLL LL||/ ////_");
           System.out.println("     L L_L_LLL|////L _LLL|//LL LLL L| // / ///_L LLL|| /// /");
           System.out.println("        _LLLLL |/////_ L|/// _LL LLL||// ///__   LL LL|// /");
           System.out.println("         LLL L||/ ///         L LL L|// // //");
           System.out.println("");
       }

       void drawRobot(){
           System.out.println("+-------------------------+");
           System.out.println("           ROBOT           ");
           System.out.println("+-------------------------+");
           System.out.println("           _____");
           System.out.println("          |     |");
           System.out.println("          | | | |");
           System.out.println("          |_____|");
           System.out.println("    ____ ___|_|___ ____");
           System.out.println("   ()___)         ()___)");
           System.out.println("   // /|           |L LL");
           System.out.println("  // / |           | L LL");
           System.out.println(" (___) |___________| (___)");
           System.out.println(" (___)   (_______)   (___)");
           System.out.println(" (___)     (___)     (___)");
           System.out.println(" (___)      |_|      (___)");
           System.out.println(" (___)  ___/___L___   | |");
           System.out.println("  | |  |           |  | |");
           System.out.println("  | |  |___________| /___L");
           System.out.println("//___LL |||     ||| //   LL");
           System.out.println("//   LL |||     ||| LL   //");
           System.out.println("LL   // |||     |||  LL //");
           System.out.println(" LL // ()__)   (__()");
           System.out.println("       ///       LLL");
           System.out.println("      ///         LLL");
           System.out.println("    _///___     ___LLL_");
           System.out.println("   |_______|   |_______|");
           System.out.println("");
       }

       void drawPanda(){
           System.out.println("+----------------------------------------------------------------+");
           System.out.println("                               PANDA                              ");
           System.out.println("+----------------------------------------------------------------+");
           System.out.println("                                      .,;;;;;;;,.");
           System.out.println("                                   ,;;;;;;;,/;;;;");
           System.out.println("                   .,aa@@@@@@@@@@@@a;;;;;/;;;,//;;;");
           System.out.println("          ..,,,.,aa@@@@@@@@@@@@@@@@@@@a;//;;;,//;;;");
           System.out.println("       ,;;;;;;;O@@@@@OO@@@@@@@@@@@@@@OOO@@@a,/;;;;'");
           System.out.println("     .;;//,;;;O@@@@OOO@@@@@@@@@@OOO@@@@OOO@@@@@a'");
           System.out.println("    .;;/,;;/;OO@@OO@@@@@@@@@@@@@@@@@@@@@@@OOO@@@@.");
           System.out.println("    ;;;/,;;//OO@@@@@@@OOO@@@@@@@@@@@OOO@@@@@@@@@@@.");
           System.out.println("    `;;//,;,OOO@@@@@@@@@OO@@@@@@@@@OO@@@@@@@@@@@@@@.");
           System.out.println("  ;.  ``````OOO@@@@@;;;;;;OO@@@@@OO;;;;;;@@@@@@O@@@@.");
           System.out.println(" .;;,       OOO@@@O;;' ~`;@@OOOOO@@;' ~`;;O@@@@@OO@@@");
           System.out.println(" ;;;;    ,  OOO@@O;;;,.,;O@@@@@@@@@O;,.,;;;O@@@@OO@@@,");
           System.out.println(" `;;'   ,;; OOO@@OO;;;;OOO(???????)OOO;;;;OO@@@@OO@@@%,");
           System.out.println("   `L   ;;; `OOO@@@@@OOOO@@L?????/@@OOOO@@@@@@@O@@@@%O@a");
           System.out.println("      L,`;'  `OOO@@@@OOO@@@@@@;@@@@@@OOO@@@@@@@@@@@%O@@@,");
           System.out.println("      .,L      `OO@@@@OO'@@@@@;@@@@@`OO@@@@@@@@@@%oO@@@O@;");
           System.out.println("    ,;;;; L   .::::OO@@OOOaaa@@@aaaOOO@@@@@@@`,;OO@@OOO@@;,");
           System.out.println("   .;;''    L:::.OOaa`@@@OO@@@@@@@OO@@@'::aOO.:;;OO@@@OO;::.");
           System.out.println("   '       .::L.OO@@@@O@::;;;;;;;;;;;;::O@O@OO.::::::::://::");
           System.out.println("          .:::.OL@@@@@@@@O@O::;;;::O@OO@O@@@@OO.:;;;;;;;;//:,");
           System.out.println("         .:/;:.OO@L@@@@@@@@@OO@OO@OO@@@@@@@@@OO.:;;;;;;;;;//:");
           System.out.println("        .://;;.OO@@@L@@@@@@@@@@O@@@@@@@@@@@@@@OO.:;;;;;;;;//:.");
           System.out.println("        ;//;;;;.O'//;;;;;;L@@@@@@@@@@@@@@@@@@@OO.:;;;;;;;;//:..");
           System.out.println("       ;//:;;;;:.//;;;;;;;;;@@@@@@@@@@@@@@@@@@OO.:;;;;;;;;;//..");
           System.out.println("       ;;//:;;;:://;;;;;;;;;@@@@@@@@@@@@@@@@@OO.:/;;;;;;;;;//..");
           System.out.println("       `;;;;;:::::::ooOOOoo@L@@@@@@@@@@@@@OOO.;;//;;;;;;;;;//.o,");
           System.out.println("       .;,,,.OOOOO@@@@@@@@@@@@L@@@@@@@@OOO.;;;//;;;;;;;;;;//;.OO,");
           System.out.println("      //;;.oO@@@@@@@@@@@@@@@@@@@LOOO.;;;;;;;;;;;;;;;;;;;;//;.oO@O,");
           System.out.println("     //;;;;O@@@@@@@@@@@@@@@OOO=;;;;//;;;;;;;;;;;;;;;;;;;//;.oO@@Oo");
           System.out.println("     //::;;O@@@@@@@@@@OOOOO=;;;;;;;//;;;;;;;;;;;;;;;////;.oO@@@@OO");
           System.out.println(" .n.n.n.n`;O@@@@@@@@@OOOOO=;;;;;;;;;;///;;;;////////';oO@@@@@@@@OO");
           System.out.println(".%%%%%%%%%,;;@@@@@@@@@=;;;;=;;;;///////////////':::::::::.a@@@@@@@");
           System.out.println("/%%%%%%%%%%.;;;;''''=:://:::::::::::::::::L::::::::::::://:.@@@@@'");
           System.out.println("/%%%%%%%%%//.;'     =:://:::::::::::::::::::L::::::::::://:.@@@@'");
           System.out.println(" /%%%%%%%%//'        =:://::::::::;:::::::::::L:::::::://:.@@@'");
           System.out.println("  /%%%%%%/             =:://:::;;:::::::::::::::L::::::::'");
           System.out.println("    ''''                 ''''''   ''''''''''''''''L''''");
           System.out.println("");
       }

       void drawHippo(){
           System.out.println("+----------------------------------------+");
           System.out.println("                   HIPPO                  ");
           System.out.println("+----------------------------------------+");
           System.out.println(" .-''''-. _    ");
           System.out.println("('    '  '0)-/)");
           System.out.println("'..____..:    L._");
           System.out.println("  Lu  u (        '-..------._");
           System.out.println("  |     /      :   '.        '--.");
           System.out.println(" .nn_nn/ (      :   '            'L");
           System.out.println("( '' '' /      ;     .             L");
           System.out.println(" ''----' 'L          :            : '.");
           System.out.println("        .'/                           '.");
           System.out.println("       / /                             '.");
           System.out.println("      /_|       )                     .L|");
           System.out.println("        |      /L                     . '");
           System.out.println("        '--.__|  '--._  ,            /");
           System.out.println("                     /'-,          .'");
           System.out.println("                    /   |        _.' ");
           System.out.println("                   (____L       /   ");
           System.out.println("                         L      L    ");
           System.out.println("                          '-'-'-'    ");
           System.out.println("");
       }

       void drawGameBoy(){
           System.out.println("+--------------------------+");
           System.out.println("          GAME BOY          ");
           System.out.println("+--------------------------+");
           System.out.println(" __________________________");
           System.out.println("| | (<OFF·ON>)           | |");
           System.out.println("|                          |");
           System.out.println("| .----------------------. |");
           System.out.println("| |  .----------------.  | |");
           System.out.println("| |  |                |  | |");
           System.out.println("| |))|                |  | |");
           System.out.println("| |  |                |  | |");
           System.out.println("| |  |                |  | |");
           System.out.println("| |  |                |  | |");
           System.out.println("| |  |                |  | |");
           System.out.println("| |  |                |  | |");
           System.out.println("| |  '----------------'  | |");
           System.out.println("| |____GAME BOY__________/ |");
           System.out.println("|                          |");
           System.out.println("|    .                     |");
           System.out.println("|  _| |_              .-.  |");
           System.out.println("|-[_   _]-       .-. (   ) |");
           System.out.println("|   |_|         (   ) '-' A|");
           System.out.println("|    '           '-' B     |");
           System.out.println("|                          |");
           System.out.println("|        ___    ___        |");
           System.out.println("|       (___)  (___)   ,., |");
           System.out.println("|       select start  ;:;: |");
           System.out.println("|                    ,;:;' /");
           System.out.println("|                   ,:;:'.'");
           System.out.println("'-----------------------`");
           System.out.println("");
       }
   }
}
