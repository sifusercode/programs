       }

       void drawCastle() {
           System.out.println("+-------------------------------------------------------------+");
           System.out.println("                            CASTLE                             ");
           System.out.println("+-------------------------------------------------------------+");
           System.out.println("                 [L");
           System.out.println("                  |L)                                ____");
           System.out.println("                  |                               __(_   )__");
           System.out.println("                  YL          ___               _(          )");
           System.out.println("                 T  L       __)  )--.          (     )-----`");
           System.out.println("                J    L   ,-(         )_         `---'");
           System.out.println("               Y/T`-._L (     (       _)                 __");
           System.out.println("               /[|   ]|  `-(__  ___)-`  |L          ,-(  __)");
           System.out.println("               | |    |      (__)       J'         (     )");
           System.out.println("   _           | |  ] |    _           /;L          `-  '");
           System.out.println("  (,,)        [| |    |    L'         /;  L");
           System.out.println("             /||.| /L |   /L         /.,-._L        ___ _");
           System.out.println("            /_|||| || |  /  L        | |{  |       (._.'_)");
           System.out.println("  L/L       | L| | '` |_ _ {|        | | U |   /L");
           System.out.println(" /v^vL/L   `|  Y | [  '-' '--''-''---'`'   | ,`^vL /L,`L");
           System.out.println("/ ,'./  L.` |[   |       [     __   L    ] |      /^vL  L");
           System.out.println(",'     `    |    |           ,`##Y.   ]    |___Y Y____,_,,_,,_");
           System.out.println("--   -----.-(] [ |   ]     o/####U|o      ]|| /`-, Y   _   Y  Y");
           System.out.println("   Y Y  --;`~T   |      }   L####U|[L _,.-(^) ,-'  _  (^)__  _");
           System.out.println("  Y  YY   ;'~~l  |   L     [|L###U'E'L  L LY-` _  (^) _Y  _");
           System.out.println(" Y  Y Y   ;L~~/L{| [      _,'-L`= = '.L_ ,`   (^)(^) (^) (^)");
           System.out.println("     --   ;L~~~/L|  _,.-'`_  `.L_..-'-  _ . ,_ Y_ Y_ _Y  _Y__");
           System.out.println("    _    _; L~~( Y``   Y (^) / `,      (^)      _   (^) (^)");
           System.out.println("   (^)  (^)`._~ /  L L  _.Y'`  _  ` --  Y - - -(^) - Y - Y -");
           System.out.println("    Y    Y    `'--..,-'`      (^)   _  -    _   Y ____");
           System.out.println("      --           _    _ --   Y   (^)   _ (^)  ===   ----");
           System.out.println("          __   -  (^)  (^)      --- Y   (^) Y");
           System.out.println("      _            Y    Y                Y     ");
           System.out.println("");
       }

       void drawSnowman(){
           System.out.println("+-------------------------------------+");
           System.out.println("                SNOWMAN                ");
           System.out.println("+-------------------------------------+");
           System.out.println("              .-------.");
           System.out.println("    *         | #     |    *");
           System.out.println("              |_______|         *");
           System.out.println("             _|=======|_             *");
           System.out.println(" *          [___________]");
           System.out.println("             .'       `.   *   | /");
           System.out.println("            :   @   @   :     L|/_");
           System.out.println("            :     <     :     //");
           System.out.println("   L~_s~-~s~s`._`...'_.'     //      *");
           System.out.println("*   `--~-~-~~s~/`''''  `-.  //");
           System.out.println("          /'/s/   _       `//");
           System.out.println("         / /s/   (_)       /");
           System.out.println("        ///s/     _        :  *");
           System.out.println("  *    ///s/     (_)      .'          *");
           System.out.println("      // |/`.           .'");
           System.out.println("   __//    .-''-------'`-.");
           System.out.println("    /|L  .'       _       `.      *");
           System.out.println("   / | L:        (_)        :");
           System.out.println("       :          _          :");
           System.out.println("       :         (_)         :");
           System.out.println("*      :          _          :     *");
           System.out.println("       :         (_)         :");
           System.out.println("        :                   :");
           System.out.println("''''''''`.               .''''''''''''");
           System.out.println("          `-._       _.-'");
           System.out.println("              `'''''`");
           System.out.println("");
       }

