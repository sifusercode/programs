   static class Forn {


       public Forn() {
       }

       void draw() {
           drawEarth();
           drawLightning();
           drawCastle();
           drawSnowman();
           drawDolphin();
           drawPenguin();
           drawGiraffe();
           drawRobot();
           drawPanda();
           drawHippo();
           drawGameBoy();
       }

       void drawEarth() {
           System.out.println("+--------------------------------------------+");
           System.out.println("                     EARTH                    ");
           System.out.println("+--------------------------------------------+");
           System.out.println("             _-o#&&*''''?d:>b`_");
           System.out.println("          _o/-`''  '',, dMF9MMMMMHo_");
           System.out.println("       .o&#'        `-MbHMMMMMMMMMMMHo.");
           System.out.println("     .o-- '         vodM*$&&HMMMMMMMMMM?.");
           System.out.println("    ,'              $M&ood,~'`(&##MMMMMMH`");
           System.out.println("   /               ,MMMMMMM#b?#bobMMMMHMMM`");
           System.out.println("  &              ?MMMMMMMMMMMMMMMMM7MMM$R*Hk");
           System.out.println(" ?$.            :MMMMMMMMMMMMMMMMMMM/HMMM|`*`");
           System.out.println("|               |MMMMMMMMMMMMMMMMMMMMbMH'   T,");
           System.out.println("$H#:            `*MMMMMMMMMMMMMMMMMMMMb#}'  `?");
           System.out.println("]MMH#             --*----*#MMMMMMMMMMMMM'    -");
           System.out.println("MMMMMb_                   |MMMMMMMMMMMP'     :");
           System.out.println("HMMMMMMMHo                 `MMMMMMMMMT       .");
           System.out.println("?MMMMMMMMP                  9MMMMMMMM}       -");
           System.out.println("-?MMMMMMM                  |MMMMMMMMM?,d-    '");
           System.out.println(" :|MMMMMM-                 `MMMMMMMT .M|.   :");
           System.out.println("  .9MMM[                    &MMMMM*' `'    .");
           System.out.println("   :9MMk                    `MMM#-        -");
           System.out.println("     &M}                     `          .-");
           System.out.println("      `&.                             .");
           System.out.println("        `~,   .                     ./");
           System.out.println("            . _                  .-");
           System.out.println("              '`--._,dd###pp=--'");
           System.out.println("");
       }

       void drawLightning() {
           System.out.println("+----------------------------------------------------+");
           System.out.println("                       LIGHTNING                      ");
           System.out.println("+----------------------------------------------------+");
           System.out.println("    .edee...      .....       .eeec.   ..eee..");
           System.out.println("   .d*-  ----*e..d*-----**e..e*--  -*c.d--  --*e.");
           System.out.println("  z-           -$          $--       *F         **e.");
           System.out.println(" z-             -c        d-          *.           -$.");
           System.out.println(".F                        -            -            'F");
           System.out.println("d                                                   J%");
           System.out.println("3         .                                        e-");
           System.out.println("4r       e-              .                        d-");
           System.out.println(" $     .d-     .        .F             z ..zeeeeed-");
           System.out.println(" -*beeeP-      P        d      e.      $**--    -");
           System.out.println("     -*b.     Jbc.     z*%e.. .$**eeeeP-");
           System.out.println("        -*beee* -$$eeed-  ^$$$--    -");
           System.out.println("                 '$$.     .$$$c");
           System.out.println("                  -$$.   e$$*$$c");
           System.out.println("                   -$$..$$P- '$$r");
           System.out.println("                    -$$$$-    -$$.           .d");
           System.out.println("        z.          .$$$-      -$$.        .dP-");
           System.out.println("        ^*e        e$$-         -$$.     .e$-");
           System.out.println("          *b.    .$$P-           -$$.   z$-");
           System.out.println("           -$c  e$$-              -$$.z$*-");
           System.out.println("            ^*e$$P-                -$$$-");
           System.out.println("              *$$                   -$$r");
           System.out.println("              '$$F                 .$$P");
           System.out.println("               $$$                z$$-");
           System.out.println("               4$$               d$$b.");
           System.out.println("               .$$%            .$$*-*$$e.");
           System.out.println("            e$$$*-            z$$-    -*$$e.");
           System.out.println("           4$$-              d$P-        -*$$e.");
           System.out.println("           $P              .d$$$c           -*$$e..");
           System.out.println("          d$-             z$$- *$b.            -*$L");
           System.out.println("         4$-             e$P-   -*$c            ^$$");
           System.out.println("         $-            .d$-       -$$.           ^$r");
           System.out.println("        dP            z$$-         ^*$e.          -b");
           System.out.println("       4$            e$P             -$$           -");
           System.out.println("                    J$F               $$");
           System.out.println("                    $$               .$F");
           System.out.println("                   4$-               $P-");
           System.out.println("                   $-               dP  ");
