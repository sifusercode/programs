               case "Translate":
                   System.out.println("ENTER STRING THAT WILL BE TRANSLATED");
                   stringtranslate = scan.nextLine();
                   if (stringtranslate.equals("String1")) {
                       System.out.println("TRANSLATE TO MORSE CODE OR BINARY");
                       String something1 = scan.nextLine();
                       if (something1.equals("morse code")) {
                           for (int i = 0; i < string1.length(); i += 1) {
                               String str = string1.substring(i, i + 1);
                               if (str.equals("a") | str.equals("A")) {
                                   str = ".－";
                                   quicksave = quicksave + str + " ";
                                   str = "";
                               } else if (str.equals("b") | str.equals("B")) {
                                   str = "－...";
                                   quicksave = quicksave + str + " ";
                                   str = "";
                               } else if (str.equals("c") | str.equals("C")) {
                                   str = "-.-.";
                                   quicksave = quicksave + str + " ";
                                   str = "";
                               } else if (str.equals("d") | str.equals("D")) {
                                   str = "－..";
                                   quicksave = quicksave + str + " ";
                                   str = "";
                               } else if (str.equals("e") | str.equals("E")) {
                                   str = ".";
                                   quicksave = quicksave + str + " ";
                                   str = "";
                               } else if (str.equals("f") | str.equals("F")) {
                                   str = "..－.";
                                   quicksave = quicksave + str + " ";
                                   str = "";
                               } else if (str.equals("g") | str.equals("G")) {
                                   str = "－－.";
                                   quicksave = quicksave + str + " ";
                                   str = "";
                               } else if (str.equals("h") | str.equals("H")) {
                                   str = "....";
                                   quicksave = quicksave + str + " ";
                                   str = "";
                               } else if (str.equals("i") | str.equals("I")) {
                                   str = "..";
                                   quicksave = quicksave + str + " ";
                                   str = "";
                               } else if (str.equals("j") | str.equals("J")) {
                                   str = ".－－－";
                                   quicksave = quicksave + str + " ";
                                   str = "";
                               } else if (str.equals("k") | str.equals("K")) {
                                   str = "－.－";
                                   quicksave = quicksave + str + " ";
                                   str = "";
                               } else if (str.equals("l") | str.equals("L")) {
                                   str = ".－..";
                                   quicksave = quicksave + str + " ";
                                   str = "";
                               } else if (str.equals("m") | str.equals("M")) {
                                   str = "－－";
                                   quicksave = quicksave + str + " ";
                                   str = "";
                               } else if (str.equals("n") | str.equals("N")) {
                                   str = "－.";
                                   quicksave = quicksave + str + " ";
                                   str = "";
                               } else if (str.equals("o") | str.equals("O")) {
                                   str = "－－－";
                                   quicksave = quicksave + str + " ";
                                   str = "";
                               } else if (str.equals("p") | str.equals("P")) {
                                   str = ".－－.";
                                   quicksave = quicksave + str + " ";
                                   str = "";
                               } else if (str.equals("q") | str.equals("Q")) {
                                   str = "－－.－";
                                   quicksave = quicksave + str + " ";
                                   str = "";
                               } else if (str.equals("r") | str.equals("R")) {
                                   str = ".－.";
                                   quicksave = quicksave + str + " ";
                                   str = "";
                               } else if (str.equals("s") | str.equals("S")) {
                                   str = "...";
                                   quicksave = quicksave + str + " ";
                                   str = "";
                               } else if (str.equals("t") | str.equals("T")) {
                                   str = "－";
                                   quicksave = quicksave + str + " ";
                                   str = "";
                               } else if (str.equals("u") | str.equals("U")) {
                                   str = "..－";
                                   quicksave = quicksave + str + " ";
                                   str = "";
                               } else if (str.equals("v") | str.equals("V")) {
                                   str = "...－";
                                   quicksave = quicksave + str + " ";
                                   str = "";
                               } else if (str.equals("w") | str.equals("W")) {
                                   str = ".－－";
                                   quicksave = quicksave + str + " ";
                                   str = "";
                               } else if (str.equals("x") | str.equals("X")) {
                                   str = "－..－";
                                   quicksave = quicksave + str + " ";
                                   str = "";
                               } else if (str.equals("y") | str.equals("Y")) {
                                   str = "－.－－";
                                   quicksave = quicksave + str + " ";
                                   str = "";
                               } else if (str.equals("z") | str.equals("Z")) {
                                   str = "－－..";
                                   quicksave = quicksave + str + " ";
                                   str = "";
                               } else if (str.equals("1")) {
                                   str = ".----";
                                   quicksave = quicksave + str + " ";
                                   str = "";
                               } else if (str.equals("2")) {
                                   str = "..---";
                                   quicksave = quicksave + str + " ";
                                   str = "";
                               } else if (str.equals("3")) {
                                   str = "...--";
                                   quicksave = quicksave + str + " ";
                                   str = "";
                               } else if (str.equals("4")) {
                                   str = "....-";
                                   quicksave = quicksave + str + " ";
                                   str = "";
                               } else if (str.equals("5")) {
                                   str = ".....";
                                   quicksave = quicksave + str + " ";
                                   str = "";
                               } else if (str.equals("6")) {
                                   str = "-....";
                                   quicksave = quicksave + str + " ";
                                   str = "";
                               } else if (str.equals("7")) {
                                   str = "--...";
                                   quicksave = quicksave + str + " ";
                                   str = "";
                               } else if (str.equals("8")) {
                                   str = "---..";
                                   quicksave = quicksave + str + " ";
                                   str = "";
                               } else if (str.equals("9")) {
                                   str = "----.";
                                   quicksave = quicksave + str + " ";
                                   str = "";
                               } else if (str.equals("0")) {
                                   str = "-----";
                                   quicksave = quicksave + str + " ";
                                   str = "";
                               } else if (str.equals(" ")) {
                                   str = "/";
                                   quicksave = quicksave + str + " ";
                                   str = "";
                               }else {
                                   quicksave = quicksave + str + " ";
                                   str = "";
                               }
                           }
                           string1 = quicksave;
                           quicksave = "";
                       } else if (something1.equals("binary")) {
                           for (int i = 0; i < string1.length(); i += 1) {
                               String str = string1.substring(i, i + 1);
                               if (str.equals("a")) {
                                   str = "01100001";
                                   quicksave = quicksave + str + " ";
                                   str = "";
                               } else if (str.equals("b")) {
                                   str = "01100010";
                                   quicksave = quicksave + str + " ";
                                   str = "";
                               } else if (str.equals("c")) {
                                   str = "1100011";
                                   quicksave = quicksave + str + " ";
                                   str = "";
                               } else if (str.equals("d")) {
                                   str = "01100100";
                                   quicksave = quicksave + str + " ";
                                   str = "";
                               } else if (str.equals("e")) {
                                   str = "01100101";
                                   quicksave = quicksave + str + " ";
                                   str = "";
                               } else if (str.equals("f")) {
                                   str = "01100110";
                                   quicksave = quicksave + str + " ";
                                   str = "";
                               } else if (str.equals("g")) {
                                   str = "01100111";
                                   quicksave = quicksave + str + " ";
                                   str = "";
                               } else if (str.equals("h")) {
                                   str = "01101000";
                                   quicksave = quicksave + str + " ";
                                   str = "";
                               } else if (str.equals("i")) {
                                   str = "01101001";
                                   quicksave = quicksave + str + " ";
                                   str = "";
                               } else if (str.equals("j")) {
                                   str = "01101010";
