           switch (f) {

               case "StorageInt":
                   System.out.println("ENTER INT FOR INT 1");
                   int1 = scan.nextInt();
                   System.out.println("ENTER INT FOR INT 2");
                   int2 = scan.nextInt();
                   break;


               case "ChangeInt":
                   System.out.println("ENTER NEW INT FOR INT 1");
                   int1 = scan.nextInt();
                   System.out.println("ENTER NEW INT FOR INT 2");
                   int2 = scan.nextInt();
                   System.out.println("The value of int1 and int2 has been replaced with " + int1 + " and " + int2);
                   break;

               case "allintAdd":
                   System.out.println("ENTER NUMBER THAT INT 1 AND INT 2 WILL ADD");
                   intquicksave = scan.nextInt();
                   int1 += intquicksave;
                   int2 += intquicksave;
                   System.out.println("Now the values of int1 and int2 are " + int1 + " and " + int2);
                   break;

               case "allintSubtract":
                   System.out.println("ENTER NUMBER THAT INT 1 AND INT 2 WILL SUBTRACT");
                   intquicksave = scan.nextInt();
                   int1 -= intquicksave;
                   int2 -= intquicksave;
                   System.out.println("Now the values of int1 and int2 are " + int1 + " and " + int2);
                   break;

               case "allintMultiply":
                   System.out.println("ENTER NUMBER THAT INT 1 AND INT 2 WILL BE MULTIPLIED BY");
                   intquicksave = scan.nextInt();
                   int1 *= intquicksave;
                   int2 *= intquicksave;
                   System.out.println("Now the values of int1 and int2 are " + int1 + " and " + int2);
                   break;

               case "allintDivide":
                   System.out.println("ENTER NUMBER THAT INT 1 AND INT 2 WILL BE DIVIDED BY");
                   intquicksave = scan.nextInt();
                   int1 /= intquicksave;
                   int2 /= intquicksave;
                   System.out.println("Now the values of int1 and int2 are " + int1 + " and " + int2);
                   break;

               case "allintModulo":
                   System.out.println("ENTER NUMBER TO FIND MODULO OF INT 1 AND INT 2");
                   intquicksave = scan.nextInt();
                   int1 = int1 % intquicksave;
                   int2 = int2 % intquicksave;
                   System.out.println("Now the values of int1 and int2 are " + int1 + " and " + int2);
                   break;

               case "StorageDouble":
                   System.out.println("ENTER DOUBLE FOR DOUBLE 1");
                   double1 = scan.nextDouble();
                   System.out.println("ENTER DOUBLE FOR DOUBLE 2");
                   double2 = scan.nextDouble();
                   break;

               case "ChangeDouble":
                   System.out.println("ENTER NEW DOUBLE FOR DOUBLE 1");
                   double1 = scan.nextDouble();
                   System.out.println("ENTER NEW DOUBLE FOR DOUBLE 2");
                   double2 = scan.nextDouble();
                   System.out.println("The value of double1 and double2 has been replaced with " + double1 + " and " + double2);
                   break;

               case "alldoubleAdd":
                   System.out.println("ENTER NUMBER THAT DOUBLE 1 AND DOUBLE 2 WILL ADD");
                   doublequicksave = scan.nextDouble();
                   double1 += doublequicksave;
                   double2 += doublequicksave;
                   System.out.println("Now the values of double1 and double2 are " + double1 + " and " + double2);
                   break;

               case "alldoubleSubtract":
                   System.out.println("ENTER NUMBER THAT DOUBLE 1 AND DOUBLE 2 WILL SUBTRACT");
                   doublequicksave = scan.nextDouble();
                   double1 -= doublequicksave;
                   double2 -= doublequicksave;
                   System.out.println("Now the values of double1 and double2 are " + double1 + " and " + double2);
                   break;

               case "alldoubleMultiply":
                   System.out.println("ENTER NUMBER THAT DOUBLE 1 AND DOUBLE 2 WILL BE MULTIPLIED BY");
                   doublequicksave = scan.nextDouble();
                   double1 *= doublequicksave;
                   double2 *= doublequicksave;
                   System.out.println("Now the values of double1 and double2 are " + double1 + " and " + double2);
                   break;

               case "alldoubleDivide":
                   System.out.println("ENTER NUMBER THAT DOUBLE 1 AND DOUBLE 2 WILL BE DIVIDED BY");
                   doublequicksave = scan.nextDouble();
                   double1 /= doublequicksave;
                   double2 /= doublequicksave;
                   System.out.println("Now the values of double1 and double2 are " + double1 + " and " + double2);
                   break;

               case "alldoubleModulo":
                   System.out.println("ENTER NUMBER TO FIND MODULO OF DOUBLE 1 AND DOUBLE 2");
                   doublequicksave = scan.nextDouble();
                   double1 = double1 % doublequicksave;
                   double2 = double2 % doublequicksave;
                   System.out.println("Now the values of double1 and double2 are " + double1 + " and " + double2);
                   break;

               case "printInt":
                   System.out.println("Int1: "+int1);
                   System.out.println("Int2: "+int2);
                   break;

               case "printDouble":
                   System.out.println("Double1: "+double1);
                   System.out.println("Double2: "+double2);
                   break;

               case "Calculator":
                   System.out.println("ENTER FIRST DOUBLE");
                   number1 = scan.nextDouble();
                   System.out.println("ENTER NUMBER FOR OPERATION. 1 for +, 2 for -, 3 for *, 4 for /, 5 for %");
                   operationsymbol = scan.nextInt();
                   System.out.println("ENTER SECOND DOUBLE");
                   number2 = scan.nextDouble();
                   if (operationsymbol == 1) {
                       answer = number1 += number2;
                       System.out.println(answer);
                   } else if (operationsymbol == 2) {
                       answer = number1 - number2;
                       System.out.println(answer);
                   } else if (operationsymbol == 3) {
                       answer = number1 * number2;
                       System.out.println(answer);
                   } else if (operationsymbol == 4) {
                       answer = number1 / number2;
                       System.out.println(answer);
                   } else if (operationsymbol == 5) {
                       answer = number1 % number2;
                       System.out.println(answer);
                   }
                   break;


               case "StorageString":
                   System.out.println("ENTER STRING FOR STRING 1");
                   string1 = scan.nextLine();
                   System.out.println("");
                   System.out.println("ENTER STRING FOR STRING 2");
