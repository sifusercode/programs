           System.out.println("                `._L         ~~--.._    `. . . . . ~-.    .- .   ,'/");
           System.out.println("_  . _ . -~L        _ ..  _          ~~--.`_. . . . . ~-_     ,-','`  .");
           System.out.println("             ` ._           ~                ~--. . . . .~=.-'. /. `");
